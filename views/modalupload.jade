div(id='modal-upload', class='modal fade')
    div(class='modal-dialog')
        div(class='modal-content')
            form(id='file-upload-form', enctype="multipart/form-data" role="form")
                div(class='modal-header')
                    button(type='button', class='close', data-dismiss='modal', aria-label='Close')
                        span(aria-hidden='true') &times;
                    h4(id='lblTitle', class='modal-title')  Upload Track
                div(class='modal-body')
                    span(class='btn btn-success fileinput-button')
                        input(id='upload-track', type='file', name='files[]')

                div(class='modal-footer')
                    div(class='col-md-12')
                        button(class="btn", data-dismiss="modal", aria-hidden="true") Cancel


